Program Vectors;
VARS
    int: x[4],y[4];
    int: a, b, c, d, e, f, g, h;

main(){

    x[0] = 10;
    x[1] = 20;
    x[2] = 30;
    x[3] = 40;
    write("Vectors started", x[0], x[1], x[2], x[3]);
    a = x[0];
    b = x[1];
    c = x[2];
    d = x[3];
    write("Assign with vectors ", a , b , c, d);
    g = 2;
    h = 3;
    y[0] = x[0];
    y[1] = x[1];
    y[2] = x[g];
    y[3] = x[h];
    write("Vectors assign with vectors started", y[0], y[1], y[2], y[3]);
}