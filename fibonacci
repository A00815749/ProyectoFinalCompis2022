Program fibonacci;
VARS
    int: i, j, next, actual, temporal;
    float: e;
function int fibo (int: j);
{ 
    if (j < 2) then {
        return(j);
    } else {
        return(fibo(j - 1) + fibo(j - 2));
    }
}
main(){
    i = fibo(7);
    write(i);

    next = 1;
    actual = 0;
    temporal = 0;
    for j = 1 to 8 do {
        temporal = actual;
        actual = next;
        next = next + temporal;
    } 

    write(" Recursion above and iteration below");
    write(actual);
}